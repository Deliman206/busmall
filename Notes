<<<<OR>>>>

===>Store Data
function storeData(){
    localStorage.imgClickData = JSON.stringify(imgClickData)
}

===> Retrieve Data
function retrieveData{
    if(localStorage.imgClickData){
        localStorage.imgClickData;
        imgClickData = JSON.parse(localStorage.imgClickData)
    }
    createRandomImages();
}